{
	"name": "GitHub similar repository suggestions",
	"version": "0.1.0",
	"manifest_version": 2,
	"description": "",
	"permissions": [
		"storage",
		"activeTab"
	 ],
	"content_scripts": [
	  {
		"matches": [
		  "*://github.com/*"
		],
		"js": ["./src/content.js"],
		"css": ["./src/popup.css"]
	  }
	],
	"web_accessible_resources": [
		"src/popup.html",
		"popup.css"
	],
	"browser_action": {},
	"background": {
	    "scripts":["./src/background.js"]
	}
}